CREATE TABLE media (
    uuid                TEXT,
    title               TEXT,
    age_restriction     TEXT,
    genre               TEXT,
    lang                TEXT,
    rating_sum          INTEGER,
    rating_count        INTEGER,
    recent_views        INTEGER,
    media_type          INTEGER
);

CREATE TABLE watchlists (
    uuid                TEXT,
    user                TEXT,
    public              BOOLEAN,
    title               TEXT,
);